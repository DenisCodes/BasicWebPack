{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "max-len": [1, 70, 2, { "ignoreComments": true }],
    "linebreak-style": 0,
    "no-console": "off",
    "indent": "off",
    "space-before-function-paren": "off",
    "prefer-destructuring": "off",
    "func-names": "off",
    "no-undef": "off"
  }
}